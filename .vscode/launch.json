    "version": "0.2.0",
    "configurations": [
        {
            "name": "dalle_mobile_client",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define", "DALLE_API=MOCK"
            ],
        },
        {
            "name": "dalle_mobile_client (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "dalle_mobile_client (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define", "DALLE_API=WEB"
            ],
        }
    ]
}